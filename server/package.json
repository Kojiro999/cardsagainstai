{
  "name": "cardsagainstai",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint . --ext .js",
    "client": "cd ../client && npm run start",
    "build:client": "sudo rm -rf build && cd ../client && npm run build --prod && cp -r build ../server",
    "startboth": "npm run server & npm run client",
    "deploy": "git push heroku master",
    "deploy:full": "npm run build:client && git add . && git commit -m fullbuild && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NauqGnesh/cardsagainstai.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NauqGnesh/cardsagainstai/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,json, css,scss,md}": [
      "prettier --write"
    ]
  },
  "homepage": "https://github.com/NauqGnesh/cardsagainstai#readme",
  "dependencies": {
    "@arition/torch-js": "^0.11.0",
    "body-parser": "^1.19.0",
    "commitizen": "^4.2.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "tokenizers": "^0.7.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./server/node_modules/cz-conventional-changelog"
    }
  }
}
